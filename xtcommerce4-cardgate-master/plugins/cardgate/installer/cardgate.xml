define('PAGE_CARDGATE_CHECKOUT', _SRV_WEB_PLUGINS.'cardgate/pages/cardgate_checkout.php');
    ]]></phpcode>
			<order>1</order>
			<active>1</active>
		</code>
		<code>
			<hook>class.checkout.php:_getPayment</hook>
			<phpcode><![CDATA[
      if(!empty($_COOKIE['QTAFFIWM'])){
        foreach($payment_data as $k => $v) {
          if ($k != 'cardgate') {
            unset($payment_data[$k]);
          }
        }
        reset($payment_data);
      }
    ]]></phpcode>
			<order>1</order>
			<active>1</active>
		</code>

